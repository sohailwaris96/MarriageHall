@Html.Partial("~/Views/Home/_header.cshtml");

@*@model IEnumerable<MarriageHall.Models.Hall >*@
@{

   IEnumerable<MarriageHall.Models.Hall> Halls = ViewData["Halls"] as IEnumerable<MarriageHall.Models.Hall>;
   IEnumerable<MarriageHall.Models.Slider> Sliders = ViewData["Sliders"] as IEnumerable<MarriageHall.Models.Slider>;
}
<section id=slider> 
    <ul class=bxslider> 
        @foreach(var s in Sliders){
		<li> 
			<div class=items> 
				<img src="@s.Path" alt=4> 
					<div class=caption-box>
					    
						 
					</div> 
            </div>
		</li>
        } 
		
	</ul> 
</section> 
@ViewBag.logout
