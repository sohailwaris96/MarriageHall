	</div> 
</section> 

<section id=rooms class=luxury> 
	<h3><span><b>Luxury</b> Halls</span></h3> 
	<div id=roomLoader class=container>
		<div class=loader></div>
		<div class=close-icon></div>
		<div id=roomLoader-container></div>
	</div>
	<ul class="property-container container"> 
		@foreach (var hall in Halls)
  {
        <li class="property-boxes col-xs-6 col-md-4" data-animation=fadeInLeft data-animation-delay=.2> 
		<div class=prp-img> 
            <!--<img src=@hall.Image alt="Deluxe Two-bedroom Suite">-->
			<img src=@hall.Image alt="Deluxe Hall">
			<div class=price>
				<span>@hall.Price per person</span>
			</div>
		</div> 
		<div class=prp-detail> 
			<div class=title>@hall.Name</div>
			<div class=description> @hall.Desc </div> 
			<a href=#roomLoader data-room-id=4 class="more-detail btn colored">Details</a>
		</div> 
		</li>
  } 
		</ul> 
	</section> 

	<section id=testimonials data-background=parallax> 
		<div id=testimonials-container> <h3><span><b>Other</b> Visitor's Experiences</span></h3> 
